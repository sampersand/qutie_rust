Fraction = class(){
   __init = func(numer, denom){
      __self.numer = numer;
      __self.denom = denom;
   };
   __text = func(){
      __self.numer + "/" + __self.denom
   };
   __add = func(other){
      3
   }   
};

frac = Fraction(1, 2);
