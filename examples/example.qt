#[include(*Plugins, *Operators, *Builtins)]



Car = class(){
   __init = func(maker, type, wheels){
      __self.maker = maker;
      __self.type = type;
      __self.wheels = wheels;
   };
   __text = func(){
      "I'm a " + __self.type +
      ", made by " +__self.maker +
      ". I have " + __self.wheels + " wheels"
   }
};

c = Car("Honda", "Car", 4);
disp(c.maker);
disp(c.wheels);
disp(c.type);
disp(c.__text());