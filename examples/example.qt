#[include(*Plugins, *Operators, *Builtins)]

Fraction = class(){
   __init = func(numer, denom){
      __self.numer = numer;
      __self.denom = denom;
   };
   __text = func(){
      "" + __self.numer + "/" + __self.denom
   };
   __add = func(other){
      if(other.__class == Fraction, 0, 1)
   };
};

frac = Fraction(1, 2);
disp(frac + 3);